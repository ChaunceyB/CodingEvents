<h1> Coding Events </h1>


<p>
    <a asp-controller="Events" asp-action="Add">Add Event</a>

@if (ViewBag.events.Count < 1)
            {
                <p>No events yet!</p>
            }
<table style="width: 50%">
    <tr>
        <th>Event</th>
        <th>Event Description</th>
    </tr>
    
        @foreach (KeyValuePair<string, string> meeting in ViewBag.events)
        {
            <tr>
                <td>@meeting.Key</td>
                <td>@meeting.Value</td>
            </tr>
        }
    

</table>


